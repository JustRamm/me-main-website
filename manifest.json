{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "src/assets/styles.css",
          "src/assets/locales/en/translation.json"
        ],
        "matches": ["<all_urls>"]
      }
    ],
    "permissions": ["activeTab"]
  }
  